.wrap{
  width: 100%;
  height: 320rpx;
  margin: 30rpx auto;
  border: 1px solid #000;
  position: relative;
}
.wrap view{position: absolute}
.wrap view:nth-of-type(1){
  left: 30%;
  top: 50rpx;
  z-index: 0
}
.wrap view:nth-of-type(2){
  left: 80%;
  top: 140rpx;
  z-index: 1;
}
.wrap view:nth-of-type(3){
  left: 10%;
  top: 220rpx;
  z-index: 3
}
